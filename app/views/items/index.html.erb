<h1>Item index</h1>
  <%= form_tag items_path, method: :get do %>
  <%= text_field_tag :query,
  params[:query],
  class: "form-control",
  placeholder: "Search for item to borrow" %>
  <%= submit_tag "Search", class: "btn btn-primary" %>
  <% end %>

  <% @items.each do |item| %>
  <%= item.item.name %>
  <%= item.storage.address %>
  <% end %>
