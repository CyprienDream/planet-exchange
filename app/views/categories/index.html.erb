<div class="container">
  <div class="top-container">
  <h2>Categories</h2>
  </div>
  <div class="categories categories-container">
      <% @categories.each do |category| %>
        <%= link_to category_path(category) do %>
          <div class="category-icon"><%= cl_image_tag category.photo.key %></div>
        <% end %>
      <% end %>
  </div>
</div>
